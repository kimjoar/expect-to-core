{
  "name": "expect-to-core",
  "version": "0.3.0",
  "description": "Core assertions for the assertion library expect-to",
  "keywords": [
    "expect",
    "assert",
    "assertions"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "deep-eql": "^0.1.3",
    "fn.name": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.0.15",
    "babel-core": "^6.0.14",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-stage-2": "^6.0.14",
    "expect-to": "^0.3.0",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "pre-commit": "^1.1.2",
    "standard": "^5.3.1",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "test": "npm run test-node && npm run test-web",
    "test-node": "mocha --compilers js:babel-core/register; exit 0",
    "test-web": "karma start karma.conf.js",
    "lint": "standard src/index.js test.js",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test-node"
  ],
  "author": "Kim Joar Bekkelund <kjbekkelund@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kjbekkelund/expect-to-core.git"
  },
  "license": "MIT"
}
